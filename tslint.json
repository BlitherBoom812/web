{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-react-hooks",
    "tslint-config-prettier"
  ],
  "rules": {
    "member-access": [true, "no-public"],
    "object-literal-sort-keys": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,

    "react-hooks-nesting": "error"
  }
}
